    "version": "0.2.0",
    "configurations": [
        {
            "name": "l",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/IslevSM/islevsm",
            // "args": ["lift", "--action=lift", "--compile-config=default", "-o", "030-lift", "020-link.bc"],
            // "args": ["recompile"],
            "stopAtEntry": false,
            // "cwd": "/work/host/binrec/projects/xalan/1",
            "cwd": "${workspaceFolder}/build/IslevSM",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}